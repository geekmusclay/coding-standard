{
    "name": "geekmusclay/coding-standard",
    "type": "phpcodesniffer-standard",
    "description": "GeekMusclay Coding Standard",
    "version": "0.0.1",
    "require": {
        "laminas/laminas-coding-standard": "^2.5"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
